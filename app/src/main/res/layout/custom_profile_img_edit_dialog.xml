<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/txt_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/colorblack"
        android:padding="10dp"
        android:fontFamily="@font/cookierun_regular"
        android:background="@drawable/bagie_background_with_all_black_line"
        android:textSize="18sp"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/string_dialog_title" />
    <TextView
        android:id="@+id/txt_no_img"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/colorblack"
        android:padding="15dp"
        android:background="@drawable/normal_ripple_edffect"
        android:fontFamily="@font/cookierun_regular"
        android:textSize="19sp"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_title"
        android:text="@string/string_no_img" />

    <TextView
        android:id="@+id/tv_return_original_profile"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/colorblack"
        android:padding="15dp"
        android:background="@drawable/normal_ripple_edffect"
        android:fontFamily="@font/cookierun_regular"
        android:textSize="19sp"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_no_img"
        android:text="@string/string_for_return_original_profile" />

    <TextView
        android:id="@+id/txt_for_goto_gallery"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/colorblack"
        android:fontFamily="@font/cookierun_regular"
        android:background="@drawable/normal_ripple_edffect"
        android:textSize="19sp"
        android:padding="15dp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/tv_return_original_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:text="@string/string_get_img_gallery" />

    <TextView
        android:id="@+id/txt_for_cancel_dialog"
        android:layout_width="0dp"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:textColor="@color/colorblack"
        android:fontFamily="@font/cookierun_regular"
        android:background="@drawable/normal_ripple_edffect"
        android:textSize="19sp"
        android:padding="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txt_for_goto_gallery"
        android:text="@string/string_cancel" />
</androidx.constraintlayout.widget.ConstraintLayout>